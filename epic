function announce(title,text,time)
    game.StarterGui:SetCore("SendNotification", {
        Title = title;
        Text = text;
        Duration = time;
    })
end


announce('Redware - Loader', 'Welcome to Redware!', 3)
wait(3)
announce('Redware - Loader', 'Running a version check.', 3)
wait(3)
announce('Redware - Loader', 'We are starting our services.', 3)
wait(3)
announce('Redware - Welcome', 'Loaded!', 3)
wait(3)
repeat wait() until game:IsLoaded()
